{
  "name": "easyimport",
	"displayName": "easyimport",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:easyimport.runEasyImport"
	],
	"main": "./extension.js",
